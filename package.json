{
  "name": "pika-ts",
  "type": "module",
  "version": "1.1.3",
  "packageManager": "pnpm@8.14.0",
  "description": "",
  "author": "Maurizio Crocci <mbcrocci@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/mbcrocci/pika-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbcrocci/pika-ts.git"
  },
  "bugs": "https://github.com/mbcrocci/pika-ts/issues",
  "keywords": [
    "pika",
    "amqp",
    "rabbitmq",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "destr": "^2.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/amqplib": "^0.10.4",
    "@types/node": "^20.10.7",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
